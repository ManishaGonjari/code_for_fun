class Solution(object):
    def judgeSquareSum(self, c):
        """
        :type c: int
        :rtype: bool
        """
    
        i = 0
        while (i <= c ** 0.5):
            m = (c - i*i) ** 0.5 
            if (int(m) == m):
                return True
            i += 1
        return False
            
